{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -i ./styles/app.css -o ./app/styles/app.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -i ./styles/app.css -o ./app/styles/app.css --watch",
    "postinstall": "remix setup node",
    "deploy": "flyctl deploy",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@mailchimp/mailchimp_marketing": "^3.0.74",
    "@prisma/client": "^3.6.0",
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "@xstate/inspect": "^0.5.2",
    "@xstate/react": "^1.6.3",
    "axios": "^0.25.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^5.0.0",
    "clsx": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.0.6",
    "remix-auth": "^2.5.0-0",
    "xstate": "^4.26.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.0.6",
    "@types/bcryptjs": "^2.4.2",
    "@types/mailchimp__mailchimp_marketing": "^3.0.3",
    "@types/node": "^17.0.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
    "postcss": "^8.4.5",
    "prisma": "^3.6.0",
    "tailwindcss": "^3.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
